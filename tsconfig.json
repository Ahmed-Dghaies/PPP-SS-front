{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],

    "baseUrl": "src",
    "paths": {
      "@ComIndex/*": ["app/Components/parameters/index/index-list/*"],
      "@ComDistributeur/*": ["app/Components/parameters/distributeur/distributeur-list/*"],
      "@ComReleveIndex/*": ["app/Components/home/Recette/releveIndex-list/*"],
      "@ComCardType/*": ["app/Components/parameters/Carte Bon/card-type-list/*"],
      "@ComCiterne/*": ["app/Components/parameters/citerne/citerne-list/*"],
      "@ComClient/*": ["app/Components/parameters/client/client-list/*"],
      "@ComShared/*": ["app/Components/Shared/*"],
      "@ComCarburant/*": ["app/Components/parameters/prixCarburant/prix-carburant-list/*"],
      "@ComRecette/*": ["app/Components/check-current-session/Recette/*"],
      "@ComSession/*": ["app/Components/check-current-session/*"],
      "@ComCarteBon/*": ["app/Components/inventaire/Carte Bon/card-bon-list/*"],
      "@ComBonValeur/*": ["app/Components/inventaire/Bon valeur/bon-valeur-list/*"],
      "@ComShowDet/*": ["app/Components/check-current-session/Recette/show-details/*"],
      "@ComRecetteCarteBMD/*": ["app/Components/check-current-session/Recette/show-details/recette-carte-bon/recette-carte-bon-more-details/*"],
      "@ComRecetteChequeMD/*": ["app/Components/check-current-session/Recette/show-details/recette-cheque/recette-cheque-more-details/*"],
      "@ComRecetteSEA/*": ["app/Components/check-current-session/Recette/show-details/recette-steg-et-autres/*"],
      "@ComStegEtAutre/*": ["app/Components/inventaire/Steg-et-autres/steg-et-autres-list/*"],
      "@ComSEAMD": ["app/Components/check-current-session/Recette/show-details/recette-steg-et-autres/recette-steg-et-autres-more-details/recette-steg-et-autres-more-details.component"],
      "@ComRecetteEspece/*": ["app/Components/check-current-session/Recette/show-details/recette-espece/*"],
      "@ComSubventionPeche/*": ["app/Components/inventaire/Subvention peche/subvention-peche-list/*"],
      "@ComSubventionPecheMD/*": ["app/Components/inventaire/Subvention Peche/subvention-peche-list/subvention-peche-more-details/*"],
      "@ComRecetteSubP/*": ["app/Components/check-current-session/Recette/show-details/recette-subvention-peche/*"],
      "@ComRecetteSubPMD/*": ["app/Components/check-current-session/Recette/show-details/recette-subvention-peche/recette-subvention-peche-more-details/*"]
    }
  }
}
